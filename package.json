{
  "name": "@minded/be-take-home-proj",
  "version": "0.1.0",
  "description": "Minded API Service",
  "author": "engineering@tryminded.com",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "version": "echo $npm_package_version",
    "prebuild": "rimraf dist",
    "build": "npm run copy:static && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"libs/**/*.ts\"",
    "start": "npm run copy:static && nest start",
    "start:dev": "npm run copy:static && nest start --watch",
    "start:debug": "npm run copy:static && nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --clear-cache && jest --detectOpenHandles",
    "test:watch": "jest --clear-cache && jest --watch",
    "test:cov": "jest --clear-cache && jest --coverage --detectOpenHandles",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --clear-cache && jest --config ./test/jest-e2e.json --runInBand --detectOpenHandles --forceExit",
    "copy:static": "cpx-fixed './{src,apps,libs}/**/*.json' 'dist/'",
    "db:recreate": "npx sequelize-cli db:drop && npx sequelize-cli db:create",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:seed": "npx sequelize-cli db:seed:all",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@nestjs/axios": "0.0.1",
    "@nestjs/common": "^8.2.4",
    "@nestjs/core": "^8.2.4",
    "@nestjs/passport": "^8.0.1",
    "@nestjs/platform-express": "^8.2.4",
    "@nestjs/sequelize": "^8.0.0",
    "@nestjs/swagger": "^5.1.5",
    "@optimizely/optimizely-sdk": "4.6.2",
    "@sendgrid/mail": "7.4.6",
    "axios": "0.21.4",
    "bcryptjs": "2.4.3",
    "cache-manager": "3.4.4",
    "cache-manager-ioredis": "^2.1.0",
    "class-transformer": "0.4.0",
    "class-validator": "0.13.1",
    "commander": "8.1.0",
    "connect-flash": "0.1.1",
    "connect-redis": "6.0.0",
    "csv": "5.5.3",
    "date-fns": "2.23.0",
    "dotenv": "10.0.0",
    "express-session": "1.17.2",
    "hot-shots": "8.5.1",
    "lodash": "4.17.21",
    "mysql2": "2.3.0",
    "nanoid": "3.1.25",
    "nest-winston": "1.6.0",
    "nestjs-console": "7.0.0",
    "nestjs-redis": "github:GyanendroKh/nestjs-redis#nest8-fix",
    "node-fetch": "^3.1.0",
    "node-mocks-http": "1.10.1",
    "passport": "0.4.1",
    "passport-http": "^0.3.0",
    "passport-jwt": "4.0.0",
    "passport-local": "1.0.0",
    "reflect-metadata": "0.1.13",
    "request-ip": "2.1.3",
    "rxjs": "7.3.0",
    "sequelize": "^6.13.0",
    "sequelize-typescript": "^2.1.2",
    "uuid": "8.3.2",
    "winston": "3.3.3"
  },
  "devDependencies": {
    "@nestjs/cli": "8.1.1",
    "@nestjs/schematics": "8.0.3",
    "@nestjs/testing": "8.0.6",
    "@types/analytics-node": "^3.1.5",
    "@types/bcryptjs": "^2.4.2",
    "@types/bluebird": "^3.5.36",
    "@types/cache-manager": "^3.4.2",
    "@types/cache-manager-ioredis": "^2.0.2",
    "@types/connect-flash": "^0.0.37",
    "@types/connect-redis": "^0.0.17",
    "@types/express": "^4.17.13",
    "@types/express-serve-static-core": "^4.17.27",
    "@types/express-session": "^1.17.4",
    "@types/jest": "^27.0.1",
    "@types/lodash": "^4.14.172",
    "@types/node": "^16.7.11",
    "@types/passport": "^1.0.7",
    "@types/passport-http": "^0.3.9",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/request-ip": "0.0.37",
    "@types/sequelize": "^4.28.10",
    "@types/smartystreets-javascript-sdk": "^1.6.4",
    "@types/sprintf-js": "^1.1.2",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.1",
    "@types/validator": "^13.6.3",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "cpx-fixed": "^1.6.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "helmet": "^4.6.0",
    "husky": "^7.0.2",
    "jest": "^27.1.0",
    "lint-staged": "^11.1.2",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "sequelize-cli": "^6.2.0",
    "supertest": "^6.1.6",
    "supertest-session": "^4.1.0",
    "swagger-ui-express": "^4.1.6",
    "ts-jest": "^27.0.5",
    "ts-loader": "^9.2.5",
    "ts-node": "^10.2.1",
    "tsconfig-paths": "^3.11.0",
    "typescript": "4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src/",
      "<rootDir>/libs/"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm test"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "prettier --write",
      "eslint --cache --fix"
    ]
  }
}
